#Amino acid trimming scripts (Cerius)

#Trim R-group and replace with Hydrogen (Exceptions: Gly and Pro)
SELECT/DESELECT_ALL -SCOPE "ALL MODELS"
SELECT/RULE  RESTART
SELECT/RESIDUE_PROPERTY_VALUE  #insert_residue_number
SELECT/RESET_PROPERTY -ATOM
SELECT/SELECT_OBJREF
SELECT/RULE  DESELECT
SELECT/ATOM_PROPERTY  NAME
SELECT/ATOM_PROPERTY_VALUE  N,HN,HA,CA,CB,C,O
SELECT/SELECT_OBJREF
EDIT/DELETE
SELECT/RULE  RESTART
SELECT/ATOM_PROPERTY_VALUE  CB
SELECT/SELECT_OBJREF
SKETCHER/EDIT_ELEMENT  H
SKETCHER/CHANGE_ELEMENT SELECTED
SELECT/DESELECT_ALL -SCOPE "ALL MODELS"
SELECT/RESIDUE_PROPERTY_VALUE  #insert_residue_number
SELECT/ATOM_PROPERTY  NAME
SELECT/ATOM_PROPERTY_VALUE  CA
SELECT/SELECT_OBJREF
SELECT/RULE  ADD
SELECT/ATOM_PROPERTY_VALUE  CB
SELECT/SELECT_OBJREF
MOVE/BOND_LENGTH  1.11

#Trim R-group and replace with CH3 (Exceptions: Gly, Pro, and Ala)
SELECT/DESELECT_ALL -SCOPE "ALL MODELS"
SELECT/RULE  RESTART
SELECT/RESIDUE_PROPERTY_VALUE  #insert_residue_number
SELECT/RESET_PROPERTY -ATOM
SELECT/SELECT_OBJREF
SELECT/RULE  DESELECT
SELECT/ATOM_PROPERTY  NAME
SELECT/ATOM_PROPERTY_VALUE  N,HN,HA,CA,CB,HB2,HB3,C,O
SELECT/SELECT_OBJREF
EDIT/DELETE
SELECT/RULE  RESTART
SELECT/ATOM_PROPERTY_VALUE  CB
SELECT/SELECT_OBJREF
SKETCHER/HYDROGEN_FILL SELECTED

#Trim N-side and replace with Hydrogen (Exceptions: Pro)
SELECT/DESELECT_ALL -SCOPE "ALL MODELS"
SELECT/RULE  RESTART
SELECT/RESIDUE_PROPERTY_VALUE  #insert_residue_number
SELECT/RESET_PROPERTY -ATOM
SELECT/ATOM_PROPERTY  NAME
SELECT/ATOM_PROPERTY_VALUE  N,HN
SELECT/SELECT_OBJREF
EDIT/DELETE
SELECT/ATOM_PROPERTY_VALUE  CA
SELECT/SELECT_OBJREF
SKETCHER/HYDROGEN_FILL SELECTED

#Add Hydrogen to C-side to form C(O)H
SELECT/DESELECT_ALL -SCOPE "ALL MODELS"
SELECT/RULE  RESTART
SELECT/RESIDUE_PROPERTY_VALUE  #insert_residue_number
SELECT/RESET_PROPERTY -ATOM
SELECT/ATOM_PROPERTY  NAME
SELECT/ATOM_PROPERTY_VALUE  C
SELECT/SELECT_OBJREF
SKETCHER/HYDROGEN_FILL SELECTED

#Trim C-side and replace with Hydrogen
SELECT/DESELECT_ALL -SCOPE "ALL MODELS"
SELECT/RULE  RESTART
SELECT/RESIDUE_PROPERTY_VALUE  #insert_residue_number
SELECT/ATOM_PROPERTY  NAME
SELECT/ATOM_PROPERTY_VALUE  O
SELECT/SELECT_OBJREF
EDIT/DELETE
SELECT/ATOM_PROPERTY_VALUE  C
SELECT/SELECT_OBJREF
SKETCHER/EDIT_ELEMENT  H
SKETCHER/CHANGE_ELEMENT SELECTED
SELECT/RULE  ADD
SELECT/ATOM_PROPERTY_VALUE  CA
SELECT/SELECT_OBJREF
MOVE/BOND_LENGTH  1.1086
SELECT/DESELECT_ALL -SCOPE "ALL MODELS"
